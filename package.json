{
	"name": "iconfont-preview",
	"displayName": "iconfont-preview",
	"description": "preview iconfont file in vscode",
	"author": {
		"name": "stxr"
	},
	"icon": "images/logo.png",
	"publisher": "stxr",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Visualization"
	],
	"keywords": [
		"ttf",
		"iconfont",
		"otf",
		"woff",
		"woff2",
		"eot"
	],
	"activationEvents": [
		"onCustomEditor:ttf.preview"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Stxr/iconfont-preview-extension"
	},
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "ttf.preview",
				"displayName": "ttf preview",
				"selector": [
					{
						"filenamePattern": "*.ttf"
					},
					{
						"filenamePattern": "*.otf"
					},
					{
						"filenamePattern": "*.woff"
					},
					{
						"filenamePattern": "*.woff2"
					},
					{
						"filenamePattern": "*.eot"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/opentype.js": "^1.3.1",
		"@types/pako": "^2.0.0",
		"@types/vscode": "^1.51.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"fonteditor-core": "^2.1.11",
		"pako": "^2.1.0"
	}
}
